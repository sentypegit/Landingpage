{
  "version": 2,
  "outputDirectory": "dist/public",
  "public": true,
  "buildCommand": "npm run build",
  "cleanUrls": true,
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
